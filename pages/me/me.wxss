/* pages/me/me.wxss */
.page-content{height: 100vh;background: #f5f5f5;}

.header{z-index: 2;height: 420rpx;position: relative;display:flex;align-items:center;justify-content:center;background: #fff}
.header .user-info-wrapper{display: flex; flex-direction: column;text-align: center;}
.header .user-info-wrapper .userPic{width: 160rpx; height: 160rpx; border-radius: 100%;border:1px solid #fff;}
.header .user-info-wrapper .userName{margin-top: 24rpx;}
.header .header-bg{position: absolute;z-index: -1;width: 100%;height: 100%;filter: blur(10px);top:0;left:0;}

.cell{height: 92rpx;line-height: 92rpx;padding: 0 20rpx;overflow: hidden;border-bottom: 1px solid #f6f6f6;}
.cell .cell-img{width: 36rpx;height: 36rpx;vertical-align: middle;margin: 28rpx 12rpx 0 0;}
.cell .more-img{width: 16px;height: 16px;vertical-align: middle;}
.top-Nav{margin-bottom: 16rpx;}
.top-Nav .top-item image{width:42rpx;height:42rpx;}
